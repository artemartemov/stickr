{
  "name": "stickr",
  "version": "1.0.0",
  "description": "A Figma plugin to organize component variants and generate sticker sheets",
  "main": "code.js",
  "scripts": {
    "build": "build-figma-plugin --typecheck --minify",
    "watch": "build-figma-plugin --typecheck --watch"
  },
  "figma-plugin": {
    "editorType": [
      "figma"
    ],
    "id": "stickr",
    "name": "Stickr",
    "main": "src/main.ts",
    "ui": "src/ui.tsx"
  },
  "keywords": ["figma", "plugin", "components", "variants", "sticker-sheet", "design-system"],
  "author": "Artem Artemov",
  "license": "MIT",
  "type": "commonjs",
  "devDependencies": {
    "@create-figma-plugin/build": "^4.0.3",
    "@create-figma-plugin/tsconfig": "^4.0.3",
    "@create-figma-plugin/ui": "^4.0.3",
    "@figma/plugin-typings": "^1.118.0",
    "typescript": "^5.9.3"
  }
}
